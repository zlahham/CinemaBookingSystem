<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="724.0" prefWidth="971.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FilmController">
   <children>
      <DatePicker fx:id="dtpckrDate" layoutX="158.0" layoutY="208.0" onAction="#showAvailableTimesOnSelectedDate" promptText="Choose date" />
      <TableView fx:id="tblTimes" layoutX="46.0" layoutY="285.0" prefHeight="318.0" prefWidth="424.0">
        <columns>
          <TableColumn fx:id="tblclmnTimesTime" minWidth="0.0" prefWidth="185.0" text="Time" />
            <TableColumn fx:id="tblclmnTimesAdd" minWidth="0.0" prefWidth="235.0" text="Add" />
        </columns>
      </TableView>
      <Button fx:id="btnBackToCustomerView" layoutX="864.0" layoutY="644.0" mnemonicParsing="false" text="Back" />
      <Button fx:id="btnLogout" layoutX="830.0" layoutY="83.0" mnemonicParsing="false" onAction="#logout" text="Log out" />
      <TableView fx:id="tblTimes2" layoutX="486.0" layoutY="285.0" prefHeight="311.0" prefWidth="413.0">
        <columns>
          <TableColumn fx:id="tblclmnTimes2Time" prefWidth="203.0" text="Time" />
          <TableColumn fx:id="tblclmnTimes2Remove" minWidth="0.0" prefWidth="209.0" text="Remove" />
        </columns>
