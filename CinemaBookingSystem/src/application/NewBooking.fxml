<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="724.0" prefWidth="971.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.BookingsController">
   <children>
      <DatePicker fx:id="dtpckrDate" layoutX="158.0" layoutY="208.0" onAction="#datePicked" promptText="Choose date" />
      <TableView fx:id="tblScreenings" layoutX="164.0" layoutY="301.0" prefHeight="200.0" prefWidth="698.0">
        <columns>
          <TableColumn fx:id="tblclmnScreeningsFilmTitle" prefWidth="266.0" text="Film Title" />
          <TableColumn fx:id="tblclmnScreeningsTime" minWidth="0.0" prefWidth="128.0" text="Time" />
            <TableColumn fx:id="tblclmnScreeningsBook" minWidth="0.0" prefWidth="412.0" text="Book" />
        </columns>
      </TableView>
      <Button fx:id="btnBackToCustomerView" layoutX="751.0" layoutY="590.0" mnemonicParsing="false" onAction="#backToUserView" text="Back" />
      <Button fx:id="btnLogout" layoutX="830.0" layoutY="83.0" mnemonicParsing="false" onAction="#logout" text="Log out" />
   </children>
</AnchorPane>
