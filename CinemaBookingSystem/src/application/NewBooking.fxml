<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="724.0" prefWidth="971.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NewBookingController">
   <children>
      <DatePicker fx:id="dtpckrDate" layoutX="158.0" layoutY="208.0" onAction="#datePicked" promptText="Choose date" />
      <TableView fx:id="tblFilms" layoutX="164.0" layoutY="301.0" prefHeight="200.0" prefWidth="537.0">
        <columns>
          <TableColumn fx:id="tblclmnFilmTitle" prefWidth="266.0" text="Film Title" />
          <TableColumn fx:id="tblclmnTime" minWidth="0.0" prefWidth="270.0" text="Time" />
        </columns>
      </TableView>
      <Button fx:id="btnBack" layoutX="751.0" layoutY="590.0" mnemonicParsing="false" onAction="#backToCustomerView" text="Back" />
      <Button fx:id="btnLogout" layoutX="830.0" layoutY="83.0" mnemonicParsing="false" onAction="#logout" text="Log out" />
   </children>
</AnchorPane>
